{
  "name": "gift_giver",
  "version": "1.0.0",
  "scripts": {
    "start": "node lib/index.js",
    "watch": "ttsc -w",
    "build": "ttsc",
    "lint": "tslint -c tslint.json 'src/**/*'",
    "test": "yarn test:unit && yarn test:integration && yarn test:contract",
    "test:env": "NODE_ENV=test",
    "test:integration": "yarn test:env jest --coverage tests/integration/*.ts",
    "test:unit": "yarn test:env jest --coverage tests/unit/*.ts",
    "test:contract": "yarn test:env jest --coverage tests/contract/*.ts"
  },
  "dependencies": {
    "express": "^4.17.1",
    "sequelize": "^5.21.5",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.8",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-transformer-imports": "^0.4.3",
    "tslint": "^6.1.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3"
  }
}
