{
  "name": "gift_giver",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "patch-package",
    "heroku-prebuild": "patch-package",
    "start": "node build/index.js",
    "watch": "ttsc --project tsconfig.build.json -w",
    "build": "ttsc --project tsconfig.build.json",
    "lint": "tslint -c tslint.json 'src/**/*'",
    "test": "yarn test:env jest --watch",
    "test:all": "yarn test:env jest --coverage tests/*/**/*.test.ts",
    "test:unit": "yarn test:env jest --coverage tests/unit/**/*.test.ts",
    "test:contract": "yarn test:env jest --coverage tests/contract/**/*.test.ts",
    "test:integration": "yarn test:env jest --coverage tests/integration/**/*.test.ts",
    "test:env": "NODE_ENV=test",
    "sequelize-cli": "ts-node node_modules/sequelize-cli/lib/sequelize --debug --config src/database/config.ts --migrations-path src/database/migrations"
  },
  "engines": {
    "node": "13.11.x"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "patch-package": "^6.2.1",
    "postinstall-postinstall": "^2.0.0",
    "sequelize": "^5.21.5",
    "sqlite3": "^4.1.1",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/joi": "^14.3.4",
    "@types/node": "^13.9.1",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.1",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "joi": "^14.3.1",
    "joi-assert": "^0.0.3",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "testdouble": "^3.13.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.0",
    "ts-transformer-imports": "^0.4.3",
    "tslint": "^6.1.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3"
  }
}
